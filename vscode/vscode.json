{
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.statusBar.feedback.visible": false,
  "typescript.experimental.syntaxFolding": true,
  "emmet.syntaxProfiles": {
    "html": {
      "filters": "bem"
    }
  },
  "emmet.triggerExpansionOnTab": true,
  "workbench.fontAliasing": "auto",
  "search.smartCase": true,
  "problems.decorations.enabled": true,
  "sassFormat.indent": 2,
  "stylelint.enable": true,
  "css.validate": false,
  "scss.validate": false,
  "html.format.indentHandlebars": true,
  "editor.tabSize": 2,
  "emmet.showAbbreviationSuggestions": false,
  "workbench.colorTheme": "Material Theme",
  "emmet.includeLanguages": {
    "javascript": "html"
  },
  "html.format.indentInnerHtml": true,
  "html.format.preserveNewLines": true,
  "html.format.maxPreserveNewLines": 1,
  "editor.fontFamily": "Operator Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 12,
  "editor.lineHeight": 22,
  "editor.letterSpacing": 0.5,
  "files.trimTrailingWhitespace": true,
  "editor.fontWeight": "400",
  "editor.renderWhitespace": "all",
  "search.useIgnoreFiles": true,
  "ruby.lint": {
    //"reek": true,
    // "rubocop": true,
    "ruby": true
    // "fasterer": true,
    // "debride": true,
    //"ruby-lint": true
  },
  "window.nativeTabs": false,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.setLocaleVariables": true,
  "terminal.integrated.fontFamily": "Menlo,'Inconsolata for Powerline','Courier New', monospace",
  "files.associations": {
    "*.thor": "ruby",
    "*.js": "javascript",
    ".stylelintrc": "json"
  },
  "workbench.iconTheme": "vscode-icons",
  "editor.minimap.enabled": false,
  "php.suggest.basic": false,
  "window.zoomLevel": 0,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "files.autoSave": "onWindowChange",
  "languageStylus.useSeparator": true,
  "editor.wordWrap": "on",
  "beautify.language": {
    "html": [
      "htm",
      "html",
      "php",
      "html.erb",
      "vue"
    ],
    "css": [
      "css"
    ]
  },
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "editor.renderIndentGuides": false,
  "vsicons.dontShowNewVersionMessage": true,
  "typescript.check.npmIsInstalled": false,
  "editor.detectIndentation": false,
  "explorer.confirmDelete": false,
  "materialTheme.cache.workbench.settings": {
    "themeColours": "Darker High Contrast",
    "accent": "Yellow",
    "accentPrevious": "Indigo"
  },
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDragAndDrop": false,
  "vsicons.projectDetection.autoReload": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": false,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
  "eslint.enable": true,
  "eslint.options": {
    "configFile": "/Users/nikodermus/.eslintrc",
    "extensions": [
      ".js",
      ".vue",
      ".jsx"
    ]
  },
  "eslint.validate": [
    "javascript",
  ],
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [{
        "settings": {
          "foreground": "#ffffffff",
        }
      },
      {
        "name": "Comment",
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#6d6d6d"
        }
      },
      {
        "name": "Constant",
        "scope": "constant",
        "settings": {
          "foreground": "#ff628c"
        }
      },
      {
        "name": "Entity",
        "scope": "entity",
        "settings": {
          "foreground": "#ffc600"
        }
      },
      {
        "name": "Invalid",
        "scope": "invalid",
        "settings": {
          "foreground": "#800f00"
        }
      },
      {
        "name": "Keyword",
        "scope": "keyword, storage.type.class.js",
        "settings": {
          "foreground": "#ff9d00"
        }
      },
      {
        "name": "Meta",
        "scope": "meta",
        "settings": {
          "foreground": "#5680f3"
        }
      },
      {
        "name": "Meta Brace",
        "scope": "meta.brace",
        "settings": {
          "foreground": "#e1efff"
        }
      },
      {
        "name": "Punctuation",
        "scope": "punctuation",
        "settings": {
          "foreground": "#e1efff"
        }
      },
      {
        "name": "Punctuation Parameters",
        "scope": "punctuation.definition.parameters",
        "settings": {
          "foreground": "#ffee80"
        }
      },
      {
        "name": "Punctuation Template Expression",
        "scope": "punctuation.definition.template-expression",
        "settings": {
          "foreground": "#ffee80"
        }
      },
      {
        "name": "Storage",
        "scope": "storage",
        "settings": {
          "foreground": "#ffc600"
        }
      },
      {
        "name": "Storage Type Arrow Function",
        "scope": "storage.type.function.arrow",
        "settings": {
          "foreground": "#ffc600"
        }
      },
      {
        "name": "String",
        "scope": [
          "string",
          "punctuation.definition.string"
        ],
        "settings": {
          "foreground": "#a5ff90"
        }
      },
      {
        "name": "String Template",
