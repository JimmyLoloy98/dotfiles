        "settings": {
          "foreground": "#FFB454"
        }
      },
      {
        "name": "[CSS] - Element Selector",
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#5680f3"
        }
      },
      {
        "name": "[CSS] - Support",
