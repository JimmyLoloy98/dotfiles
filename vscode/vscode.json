        "settings": {
          "foreground": "#ff9d00"
        }
      },
      {
        "name": "Support Variable Property DOM",
        "scope": "support.variable.property.dom",
        "settings": {
          "foreground": "#ff9d00"
        }
      },
      {
        "name": "Variable",
        "scope": "variable",
        "settings": {
          "foreground": "#e1efff"
        }
      },
      {
        "name": "[CSS] - Entity",
        "scope": [
          "source.css entity",
          "source.stylus entity"
