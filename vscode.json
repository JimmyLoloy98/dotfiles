{
  "TodoParser": {
    "folderExclude": [
      "node_modules",
      ".vscode",
      ".git"
    ],
    "include": [
      "js",
      "css",
      "scss",
      "styl",
      "html",
      "ts",
      "rb",
      "jsx",
      "vue"
    ],
  },
  "html.format.indentHandlebars": true,
  "editor.tabSize": 2,
  "emmet.showAbbreviationSuggestions": false,
  "workbench.colorTheme": "Material Theme",
  "emmet.includeLanguages": {
    "javascript": "html"
  },
  "html.format.indentInnerHtml": true,
  "html.format.preserveNewLines": true,
  "html.format.maxPreserveNewLines": 1,
  "editor.fontFamily": "Operator Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 12,
  "editor.lineHeight": 22,
  "editor.letterSpacing": 0.5,
  "files.trimTrailingWhitespace": true,
  "editor.fontWeight": "400",
  "editor.renderWhitespace": "all",
  "search.useIgnoreFiles": true,
  "ruby.lint": {
    //"reek": true,
    // "rubocop": true,
    "ruby": true
    // "fasterer": true,
    // "debride": true,
    //"ruby-lint": true
  },
  "window.nativeTabs": false,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.setLocaleVariables": true,
  "terminal.integrated.fontFamily": "Menlo,'Inconsolata for Powerline','Courier New', monospace",
  "files.associations": {
    "*.thor": "ruby",
    "*.js": "javascript"
  },
  "workbench.iconTheme": "vscode-icons",
  "editor.minimap.enabled": false,
  "php.suggest.basic": false,
  "window.zoomLevel": 0,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "files.autoSave": "onWindowChange",
  "languageStylus.useSeparator": false,
  "editor.wordWrap": "on",
  "beautify.language": {
    "html": [
      "htm",
      "html",
      "php",
      "html.erb",
      "vue"
    ],
    "css": [
      "css"
    ]
  },
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "editor.renderIndentGuides": false,
  "vsicons.dontShowNewVersionMessage": true,
  "typescript.check.npmIsInstalled": false,
  "editor.detectIndentation": false,
  "explorer.confirmDelete": false,
  "workbench.panel.location": "bottom",
  "materialTheme.cache.workbench.settings": {
    "themeColours": "Darker High Contrast",
    "accent": "Purple",
    "accentPrevious": "Graphite"
  },
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#AB47BC",
    "list.activeSelectionForeground": "#AB47BC",
    "list.inactiveSelectionForeground": "#AB47BC",
    "list.highlightForeground": "#AB47BC",
    "scrollbarSlider.activeBackground": "#AB47BC50",
    "editorSuggestWidget.highlightForeground": "#AB47BC",
    "textLink.foreground": "#AB47BC",
    "progressBar.background": "#AB47BC",
    "pickerGroup.foreground": "#AB47BC",
    "tab.activeBorder": "#AB47BC"
  },
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDragAndDrop": false,
  "vsicons.projectDetection.autoReload": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": false,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
  "eslint.enable": true,
  "eslint.options": {
    "extensions": [
      ".html",
      ".js",
      ".vue",
      ".jsx"
    ]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "html",
      "autoFix": true
    }
  ],
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": true,
}